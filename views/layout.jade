doctype html
html
  head
    title= title
    include header
  body
      div.uk-container.uk-container-center
          div.uk-grid
              div.uk-width-small-1-1
                  h1 RSS Fetch
          div.uk-grid
              div.uk-width-small-1-1
                  nav.uk-navbar
                      ul.uk-navbar-nav
                        each item,index in menus
                            if index == activeMenu
                                li.uk-active
                                    a(href= item.url,title= item.title)= item.title
                            else
                                li
                                    a(href= item.url,title= item.title)= item.title
          block content
    //include footer